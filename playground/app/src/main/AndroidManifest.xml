<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.foodiestudio.boilerplate.playground">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Playground">
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>

        <activity
            android:name=".PhotoPreviewActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/Theme.Playground.Activity" />

        <activity
            android:name=".RotateActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".aidl.AidlRemoteService"
            android:process=":haha" />

        <activity
            android:name=".aidl.ClientActivity"
            android:exported="true"></activity>
    </application>

</manifest>